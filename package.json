{
  "name": "hyperactiv",
  "version": "0.3.2",
  "description": "Super small observable & reactive objects library.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "repository": "https://github.com/elbywan/hyperactiv",
  "bugs": {
    "url": "https://github.com/elbywan/hyperactiv/issues"
  },
  "files": [
    "src",
    "dist",
    "handlers",
    "react",
    "mixins",
    "websocket"
  ],
  "scripts": {
    "start": "npm run lint && npm run build && npm run test",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "build": "rollup -c config/rollup.config.js && npm run build:handlers && npm run build:react && npm run build:mixins && npm run build:websocket",
    "build:handlers": "rollup -c config/rollup.handlers.config.js",
    "build:react": "rollup -c config/rollup.react.config.js",
    "build:mixins": "rollup -c config/rollup.mixins.config.js",
    "build:websocket": "rollup -c config/rollup.websocket.config.js",
    "test": "rollup -c config/rollup.config.js -o ./dist/hyperactiv.map.js -m inline && rollup -c config/rollup.handlers.config.js -o ./handlers/handlers.map.js -m inline && jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "clean": "rimraf {dist,react,handlers}"
  },
  "keywords": [
    "computed properties",
    "computed",
    "reactive",
    "observable",
    "observe"
  ],
  "author": "Julien Elbaz",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3"
  },
  "dependencies": {
    "rollup-plugin-terser": "^5.0.0"
  }
}
