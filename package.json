{
  "name": "hyperactiv",
  "version": "0.6.2",
  "description": "Super small observable & reactive objects library.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "repository": "https://github.com/elbywan/hyperactiv",
  "bugs": {
    "url": "https://github.com/elbywan/hyperactiv/issues"
  },
  "files": [
    "src",
    "dist",
    "handlers",
    "react",
    "classes",
    "websocket"
  ],
  "scripts": {
    "start": "npm run lint && npm run build && npm run test",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint --fix ./src ./test",
    "build": "rollup -c config/rollup.config.js && npm run build:handlers && npm run build:react && npm run build:classes && npm run build:websocket",
    "build:handlers": "rollup -c config/rollup.handlers.config.js",
    "build:react": "rollup -c config/rollup.react.config.js",
    "build:classes": "rollup -c config/rollup.classes.config.js",
    "build:websocket": "rollup -c config/rollup.websocket.config.js",
    "build:test": "env TEST_BUILD=1 npm run build && npm run build:handlers && npm run build:classes && npm run build:websocket && npm run build:react",
    "test": "npm run build:test && jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "clean": "rimraf {dist,react,handlers,websocket,classes}"
  },
  "keywords": [
    "computed properties",
    "computed",
    "reactive",
    "observable",
    "observe"
  ],
  "author": "Julien Elbaz",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "node-fetch": "^2.6.0",
    "normaliz": "^0.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "rollup": "^1.12.3",
    "rollup-plugin-terser": "^5.0.0",
    "wretch": "^1.5.2",
    "ws": "^7.0.0"
  }
}
